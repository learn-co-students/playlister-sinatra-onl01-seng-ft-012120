<h1><%= @genre.name %></h1>


<% @genre.artists.each do |artist| %>
    <p> <%= artist.name %></p>
    <a href='/artists/<%= artist.slug %>'>artist page</a>
<% end %>

<% @genre.songs.each do |song| %>
    <p> <%= song.name %></p>
    <a href='/songs/<%= song.slug %>'>song page</a>
<% end %>